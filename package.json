{
  "name": "svelte-app",
  "version": "0.2.0",
  "private": true,
  "description": "A short example of working Svelte+Prisma serverless app",
  "keywords": [
    "example",
    "short",
    "svelte",
    "prisma",
    "serverless",
    "vercel"
  ],
  "license": "MIT",
  "author": {
    "name": "Karkael",
    "email": "karkael@gmail.com",
    "url": "https://github.com/karkael64"
  },
  "scripts": {
    "build": "rollup -c && yarn prisma:seed",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "pretty": "prettier --write .",
    "svelte-check": "svelte-check",
    "prisma:seed": "ts-node --project ./scripts/tsonfig.json ./scripts/seed.ts",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push --preview-feature",
    "prisma:reset": "yarn prisma:generate && yarn prisma:push && yarn prisma:seed",
    "api": "ts-node --project ./scripts/tsonfig.json ./scripts/serverless.ts",
    "test:psql": "ts-node --project ./scripts/tsonfig.json ./scripts/testPsql.ts",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o public",
    "precodegen": "tsc scripts/codegen.ts --esModuleInterop --moduleResolution node --module umd --target es6",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@tsconfig/svelte": "^1.0.10",
    "bcrypt": "^5.0.1",
    "express-graphql": "^0.12.0",
    "graphql-tag": "^2.11.0",
    "graphql-tools": "^7.0.4",
    "pg": "^8.6.0",
    "svelte-apollo": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@graphql-codegen/cli": "1.21.4",
    "@graphql-codegen/fragment-matcher": "2.0.1",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "^1.22.0",
    "@graphql-codegen/typescript-operations": "^1.17.16",
    "@prisma/client": "^2.21.2",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-svelte-csf": "^1.0.0",
    "@storybook/svelte": "^6.2.9",
    "@types/bcrypt": "^3.0.1",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.3",
    "@types/pg": "^7.14.11",
    "babel-loader": "^8.2.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "graphql-codegen-svelte-apollo": "^0.1.5",
    "prettier": "^2.2.1",
    "prisma": "^2.21.2",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "svelte": "^3.37.0",
    "svelte-check": "^1.5.2",
    "svelte-loader": "^3.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "vercel": "^22.0.0"
  },
  "engines": {
    "node": ">=12",
    "yarn": ">=1.12"
  },
  "repository": "github:karkael64/svelte-test",
  "bugs": "https://github.com/karkael64/svelte-test/issues"
}
