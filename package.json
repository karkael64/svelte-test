{
  "name": "svelte-app",
  "version": "0.2.0",
  "private": true,
  "description": "A short example of working Svelte+Prisma serverless app",
  "keywords": [
    "example",
    "short",
    "svelte",
    "prisma",
    "serverless",
    "vercel"
  ],
  "license": "MIT",
  "author": {
    "name": "Karkael",
    "email": "karkael@gmail.com",
    "url": "https://github.com/karkael64"
  },
  "scripts": {
    "build": "rollup -c && yarn prisma:seed",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "pretty": "prettier --write .",
    "svelte-check": "svelte-check",
    "prisma:seed": "ts-node --project ./scripts/tsonfig.json ./scripts/seed.ts",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push --preview-feature",
    "prisma:reset": "yarn prisma:generate && yarn prisma:push && yarn prisma:seed",
    "api": "ts-node --project ./scripts/tsonfig.json ./scripts/serverless.ts",
    "test:psql": "ts-node --project ./scripts/tsonfig.json ./scripts/testPsql.ts"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "pg": "^8.6.0",
    "sirv-cli": "^1.0.0"
  },
  "devDependencies": {
    "@prisma/client": "^2.21.2",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@types/bcrypt": "^3.0.1",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.3",
    "@types/pg": "^7.14.11",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "prettier": "^2.2.1",
    "prisma": "^2.21.2",
    "rollup": "^2.3.4",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.5.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "vercel": "^22.0.0"
  },
  "engines": {
    "node": ">=12",
    "yarn": ">=1.12"
  },
  "repository": "github:karkael64/svelte-test",
  "bugs": "https://github.com/karkael64/svelte-test/issues"
}
